{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "summaries",
  "type": "object",
  "properties": {
    "periodId": {
      "title": "id",
      "readOnly": true,
      "writeOnly": true,
      "type": "integer"
    },
    "supplierName": {
      "title": "supplier",
      "readOnly": true,
      "type": "string"
    },
    "totalDueAmount": {
      "title": "invoices",
      "readOnly": true,
      "type": "number",
      "format": "currency"
    },
    "totalPaidAmount": {
      "title": "payments",
      "readOnly": true,
      "type": "number",
      "format": "currency"
    },
    "outstandingAmount": {
      "title": "residual",
      "readOnly": true,
      "type": "number",
      "format": "currency"
    }
  }
}
